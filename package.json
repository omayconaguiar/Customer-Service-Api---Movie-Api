{
  "name": "clean-node-api",
  "version": "1.3.0",
  "description": "NodeJs Rest API using TDD, Clean Architecture and Typescript",
  "author": "Maycon Aguiar",
  "license": "ISC",
  "repository": "https://github.com/omayconaguiar/Covid-Statistic",
  "scripts": {
    "start": "ts-node-dev src/main/server.ts",
    "build": "rimraf dist && tsc",
    "debug": "nodemon -L --watch ./dist --inspect=0.0.0.0:9222 --nolazy ./dist/main/server.js",
    "up": "npm run build && docker-compose up -d",
    "down": "docker-compose down",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "debug": "node --inspect -r ts-node/register ./src/index.ts",
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "test:unit": "npm test -- --watch -c jest-unit-config.js",
    "test:integration": "npm test -- --watch -c jest-integration-config.js",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "^1.2.3",
    "@types/bcrypt": "^3.0.0",
    "@types/bull": "^3.11.0",
    "@types/express": "^4.17.4",
    "@types/express-serve-static-core": "^4.17.14",
    "@types/faker": "^5.1.4",
    "@types/jest": "^24.9.1",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/mongodb": "^3.5.4",
    "@types/node": "^13.11.0",
    "@types/supertest": "^2.0.8",
    "@types/validator": "^12.0.1",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "bson-objectid": "^1.3.1",
    "copyfiles": "^2.4.1",
    "coveralls": "^3.1.0",
    "eslint": "^7.14.0",
    "eslint-config-standard-with-typescript": "^19.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^5.0.0",
    "faker": "^5.1.0",
    "git-commit-msg-linter": "^2.4.2",
    "husky": "^4.2.3",
    "jest": "^25.2.7",
    "lint-staged": "^10.5.2",
    "mockdate": "^3.0.2",
    "rimraf": "^3.0.2",
    "sucrase": "^3.12.1",
    "supertest": "^4.0.2",
    "ts-jest": "^24.3.0",
    "ts-node-dev": "^1.1.6",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bcrypt": "^3.0.8",
    "bull": "^3.11.0",
    "bull-board": "^0.5.0",
    "bullmq": "^1.14.8",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "mongo-round": "^1.0.0",
    "mongodb": "^3.5.5",
    "mongodb-memory-server": "6.5.1",
    "nodemon": "^2.0.2",
    "swagger-router": "^0.7.4",
    "swagger-ui-express": "^4.1.5",
    "validator": "^12.2.0"
  },
  "_moduleAliases": {
    "@": "src"
  },
  "config": {
    "mongodbMemoryServer": {
      "version": "4.4.1"
    }
  }
}
